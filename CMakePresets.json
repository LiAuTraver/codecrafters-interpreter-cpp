    },
		{
			"name": "Windows-LLVM-MSVC-Compat-Mode-Debug",
			"hidden": false,
			"inherits": [
				"LLVM-MSVC-Compat-Mode",
				"Windows",
				"vcpkg"
			],
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Debug"
			}
		},
		{
