{
	"version": 3,
	"cmakeMinimumRequired": {
		"major": 3,
		"minor": 10,
		"patch": 0
	},
	"configurePresets": [
		{
			"name": "Environment",
			"hidden": true,
			"binaryDir": "${sourceDir}/build/${presetName}",
			"cacheVariables": {
				"CMAKE_CXX_EXTENSIONS": "ON",
				"CMAKE_CXX_STANDARD": "23",
				"CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
				"CMAKE_SUPPRESS_DEVELOPER_WARNINGS": "TRUE",
				"CMAKE_BUILD_TYPE": "Debug",
				"CMAKE_CXX_FLAGS": "-D_DEBUG -DDEBUG -D__DEBUG__",
				"BUILD_SHARED_LIBS": "ON"
			}
		},
		{
			"name": "Default",
			"hidden": true,
			"generator": "Ninja",
			"cacheVariables": {
				"CMAKE_MAKE_PROGRAM": "ninja"
			},
			"inherits": [
				"Environment"
			]
		},
		{
			"name": "Visual-Studio",
			"hidden": true,
			"description": "Generates Visual Studio 2022 solution files. use `cmake --open` to open the solution",
			"generator": "Visual Studio 17 2022",
			"cacheVariables": {
				"CMAKE_CONFIGURATION_TYPES": "Debug;Release",
				"CMAKE_GENERATOR_PLATFORM": "x64"
			},
			"inherits": [
				"Environment"
			]
		},
		{
			"name": "Windows",
			"hidden": true,
			"inherits": [
				"Default"
			],
			"cacheVariables": {
				"CMAKE_SYSTEM_NAME": "Windows"
			}
		},
		{
			"name": "Linux",
			"hidden": true,
			"inherits": [
				"Default"
			],
			"cacheVariables": {
				"CMAKE_SYSTEM_NAME": "Linux"
			}
		},
		{
			"name": "Qt6",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_AUTOMOC": "ON",
				"CMAKE_AUTORCC": "ON",
				"CMAKE_AUTOUIC": "ON",
				"QT_VERSION_MAJOR": "6"
			}
		},
		{
			"name": "vcpkg-Windows",
			"hidden": true,
			"cacheVariables": {
				"CMAKE_TOOLCHAIN_FILE": {
					"value": "M:/vcpkg/scripts/buildsystems/vcpkg.cmake",
					"type": "FILEPATH"
				},
        "VCPKG_MANIFEST_MODE": "OFF",
        "VCPKG_MANIFEST_INSTALL": "OFF"
			}
		},
		{
			"name": "vcpkg-Linux",
			"hidden": true,
